<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="spedread">
	<schema id="com.github.Darazaki.Spedread" path="/com/github/Darazaki/Spedread/">
	  <key name="milliseconds-per-word" type="u">
	    <summary>Milliseconds Per Word</summary>
	    <description>
	      Controls the speed at which words are read in milliseconds per word
	    </description>
	    <default>150</default>
	  </key>
	  <key name="reading-font" type="s">
	    <summary>A</summary>
	    <description>
	      A
	    </description>
	    <default>"Arial 10"</default>
	  </key>
	</schema>
</schemalist>
